{
  "license": "UNLICENCED",
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "accordion-js": "^3.3.4",
    "bootstrap": "^5.3.1",
    "browser-sync": "^2.28.1",
    "browser-sync-webpack-plugin": "^2.3.0",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.6.0",
    "gulp": "^4.0.2",
    "gulp-imagemin": "^7.1.0",
    "import-glob-loader": "^1.1.0",
    "laravel-mix": "^6.0.49",
    "node-sass-glob-importer": "^5.3.2",
    "photoswipe": "^5.4.2",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.64.2",
    "sass-loader": "^13.3.2",
    "swiper": "^11.0.5"
  },
  "scripts": {
    "start": "concurrently \"yarn mix:watch\" \"yarn svg\" \"yarn images\"",
    "images": "yarn images:watch",
    "images:watch": "chokidar \"src/images/\" -c \"yarn images:dev\"",
    "images:dev": "yarn images:clean && yarn images:build",
    "images:clean": "rm -rf dist/img/*",
    "images:build": "cp  -r src/images/* dist/img",
    "svg": "yarn svg:watch",
    "svg:watch": "chokidar \"src/svg\" -c \"yarn svg:dev\"",
    "svg:dev": "yarn svg:clean && yarn svg:build",
    "svg:clean": "rm -f src/svg/icons/._* && rm -rf dist/svg/* && truncate -s 0 src/styles/config/_icons.scss",
    "svg:build": "gulp svg && for file in dist/svg/icons/*.svg; do echo \"$\"\"$(basename \"$file\" .svg): '$(cat \"$file\" | base64)';\" >> src/styles/config/_icons.scss; done",
    "mix": "yarn mix:watch",
    "mix:dev": "mix",
    "mix:watch": "mix watch",
    "mix:production": "mix --production"
  },
  "dependencies": {
    "aos": "^3.0.0-beta.6",
    "gsap": "^3.12.3",
    "headroom.js": "^0.12.0",
    "lenis": "^1.1.20",
    "lottie-web": "^5.12.2",
    "simple-parallax-js": "^5.6.2",
    "simplelightbox": "^2.14.3",
    "split-type": "^0.3.4"
  }
}
